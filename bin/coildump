#!/usr/bin/env python

import sys
import coil

def main():
    if len(sys.argv) != 2:
        print >>sys.stderr, "Usage: coildump <path>"
        sys.exit(2)
    try:
        print coil.parse_file(sys.argv[1])
    except Exception, ex:
        sys.stderr.write("%s\n" % ex)
        sys.exit(1)

if __name__ == '__main__':
    main()
